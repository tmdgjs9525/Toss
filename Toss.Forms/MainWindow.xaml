<james:JamesWindow
    x:Class="Toss.Forms.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:james="clr-namespace:Jamesnet.Wpf.Controls;assembly=Jamesnet.Wpf"
    xmlns:local="clr-namespace:Toss.Forms"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:names="clr-namespace:Toss.Core.Names;assembly=Toss.Core"
    Title="MainWindow"
    Width="400"
    Height="650"
    d:DesignHeight="650"
    d:DesignWidth="400"
    mc:Ignorable="d"
    Background="#101012">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>

        <james:JamesRegion Grid.Row="0" RegionName="{x:Static names:RegionNameManager.MainRegion}" />

    </Grid>
</james:JamesWindow>
